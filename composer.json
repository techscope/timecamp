{
    "name": "techscope/Timecamp",
    "type": "library",
    "description": "TimeCamp API wrapper for Laravel framework",
    "keywords": [
        "techscope",
        "Timecamp"
    ],
    "homepage": "https://github.com/techscope/Timecamp",
    "license": "MIT",
    "authors": [
        {
            "name": "TechScope LLC",
            "email": "christian.soseman@techscopellc.com",
            "homepage": "https://github.com/techscope",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "ext-libxml": ">=2.6.21",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "~1.18",
        "vlucas/phpdotenv": ">=1.0 <2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Techscope\\Timecamp\\": "src",
            "Techscope\\Timecamp\\Model\\": "src/model",
            "Techscope\\TimecampTest\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Techscope\\TimecampTest\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
